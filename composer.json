{
  "name": "violinist-dev/symfony-cloud-security-checker",
  "description": "A wrapper for the symfony cli security checker",
  "license": "MIT",
  "authors": [
    {
      "name": "Eirik S. Morland",
      "email": "eirik@morland.no"
    }
  ],
  "autoload": {
    "psr-4": {
      "Violinist\\SymfonyCloudSecurityChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Violinist\\SymfonyCloudSecurityChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "symfony/process": "^2.7 || ^3.0 || ^4.0"
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit",
      "./vendor/bin/phpcs -p -n"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7 || ^8 || ^9",
    "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "^3.3"
  }
}
